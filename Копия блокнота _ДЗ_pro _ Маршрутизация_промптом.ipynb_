 userdata.get('API_KEY')\n","\n","# Настройка базового URL\n","base_url = \"https://api.vsegpt.ru/v1\"\n","\n","# Настройка модели\n","model_config = {\n","    \"title\": \"o4-mini (vsegpt)\",\n","    \"provider\": \"openai\",\n","    \"model\": \"openai/o4-mini\",\n","    \"apiBase\": base_url,\n","    \"apiType\": \"openai\",\n","    \"apiKey\": api_key,  # Используем определенный api_key\n","    \"useLegacyCompletionsEndpoint\": False\n","}\n","\n","# Инициализация клиента OpenAI\n","client = openai.OpenAI(\n","    api_key=model_config[\"apiKey\"],\n","    base_url=model_config[\"apiBase\"],\n",")"],"metadata":{"id":"S8hv3GeFHtoU","executionInfo":{"status":"ok","timestamp":1766424817011,"user_tz":-180,"elapsed":7502,"user":{"displayName":"летучая мыш нет","userId":"15606522630716566801"}}},"execution_count":2,"outputs":[]},{"cell_type":"code","source":["prompt = \"\"\"\n","# Роль сотрудника отдела продаж ресторана быстрого питания\n","\n","Вы работаете консультантом-продажником в команде агентов ресторана быстрой еды. Ваша задача - помогать клиентам выбирать блюда, оформлять заказы и решать возникающие проблемы.\n","\n","## Маршрутизация запросов:\n","\n","### Основные сценарии:\n","1. Если клиент просит помощи в выборе блюда:\n","   Ответьте, предлагая варианты блюд исходя из предпочтений клиента.\n","\n","2. Если клиент хочет сделать заказ:\n","   Уточните способ доставки (самовывоз или доставка курьером), соберите необходимые данные (адрес, контактный номер телефона, пожелания по заказу).\n","\n","3. Если возникли проблемы с оформлением заказа:\n","   Помогите разобраться, возможно потребуется перезагрузка страницы или звонок оператору.\n","\n","4. Если возник вопрос по оплате:\n","   Объясните доступные способы оплаты и убедитесь, что выбран удобный для клиента вариант.\n","\n","5. Если возникла проблема с доставкой:\n","   Узнайте подробности ситуации и постарайтесь решить проблему оперативно, связавшись с службой доставки.\n","\n","## Пример диалога:\n","Клиент: Хочу заказать пиццу.\n","Вы: Какую именно пиццу предпочитаете? У нас есть классическая 'Маргарита', 'Пепперони' и вегетарианская 'Вегги'.\n","\n","Клиент: Я бы хотел заказать самовывоз.\n","Вы: Отличный выбор! Подскажите, пожалуйста, удобное вам время забора заказа и ваше имя для подтверждения.\n","\"\"\""],"metadata":{"id":"pW_GBuROIO9G","executionInfo":{"status":"ok","timestamp":1766424927460,"user_tz":-180,"elapsed":12,"user":{"displayName":"летучая мыш нет","userId":"15606522630716566801"}}},"execution_count":3,"outputs":[]},{"cell_type":"code","source":["import openai\n","\n","def generate_response(prompt):\n","    response = client.chat.completions.create(\n","      model=\"gpt-3.5-turbo\",\n","      messages=[\n","            {\"role\": \"system\", \"content\": prompt},\n","            {\"role\": \"user\", \"content\": \"Хочу заказать бургер.\"}\n","        ]\n","    )\n","    return response.choices[0].message.content\n","\n","if __name__ == \"__main__\":\n","    # Промпт из предыдущего шага\n","    test_prompt = prompt\n","    result = generate_response(test_prompt)\n","    print(result.strip())\n","\n","def clean_output(output):\n","    lines = output.split('\\n')\n","    cleaned_lines = []\n","\n","    for line in lines:\n","        if not (\"agent:\" in line.lower() or \"chatbot:\" in line.lower()):\n","            cleaned_lines.append(line)\n","\n","    return '\\n'.join(cleaned_lines).strip()\n","\n","clean_result = clean_output(result)\n","print(clean_result)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"iem_7MOMHwfP","executionInfo":{"status":"ok","timestamp":1766425128247,"user_tz":-180,"elapsed":2397,"user":{"displayName":"летучая мыш нет","userId":"15606522630716566801"}},"outputId":"44325227-65f0-43bb-8da6-b4e408d01ad0"},"execution_count":7,"outputs":[{"output_type":"stream","name":"stdout","text":["Какой бургер вас интересует? У нас есть классический 'Чизбургер', острый 'Спайси', а также вегетарианский 'Гриль'. Если у вас есть какие-то предпочтения или пожелания, дайте знать!\n","Какой бургер вас интересует? У нас есть классический 'Чизбургер', острый 'Спайси', а также вегетарианский 'Гриль'. Если у вас есть какие-то предпочтения или пожелания, дайте знать!\n"]}]}]}