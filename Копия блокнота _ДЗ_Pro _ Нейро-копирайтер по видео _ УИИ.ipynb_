rmat': 'm4a/bestaudio/best',\n","                'postprocessors': [{'key': 'FFmpegExtractAudio',\n","                                    'preferredcodec': 'm4a'}],\n","                'outtmpl': path_file}\n","    try:\n","        with yt_dlp.YoutubeDL(ydl_opts) as ydl:\n","            error_code = ydl.download(url)\n","            if error_code == 0:\n","                print('Загрузка файла прошла успешно!')\n","            else:\n","                print(f'Код ошибки: {error_code}')\n","        print(f\"\\nАудиофайл загружен: {path_file}.m4a\")\n","        return f'{path_file}.m4a'\n","    except:\n","        return None\n","\n","\n","# Информация об аудио файлe\n","def audio_info(audio_file):\n","    audio = AudioSegment.from_file(audio_file)\n","    print(f'\\nПродолжительность: {audio.duration_seconds / 60} min.')\n","    print(f'Частота дискретизаци: {audio.frame_rate}')\n","    print(f'Количество каналов: {audio.channels}')\n","    return audio"],"metadata":{"id":"wZqDmcEqzmDu","executionInfo":{"status":"ok","timestamp":1759564604513,"user_tz":-180,"elapsed":10,"user":{"displayName":"летучая мыш нет","userId":"15606522630716566801"}}},"execution_count":32,"outputs":[]},{"cell_type":"code","source":["# Пример: Получаем Аудио из Youtube видео\n","url= 'https://www.youtube.com/watch?v=LRuszWRN4uI&list=PLv_mO3iQ2o2fZllHFsEniuy1D-2IXr8eU'\n","folder = '/content/'\n","audio_file_name = 'audio_yt'\n","\n","audio_file = get_audio_from_video(url, folder, audio_file_name)\n","_ = audio_info(audio_file)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"collapsed":true,"id":"6G3xouwfWSkX","executionInfo":{"status":"ok","timestamp":1759564700494,"user_tz":-180,"elapsed":94027,"user":{"displayName":"летучая мыш нет","userId":"15606522630716566801"}},"outputId":"9571d9ba-e14b-4dbd-ffed-f4d1eb9d2b59"},"execution_count":33,"outputs":[{"output_type":"stream","name":"stdout","text":["[youtube:tab] Extracting URL: https://www.youtube.com/watch?v=LRuszWRN4uI&list=PL